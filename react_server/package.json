{
  "name": "react-ssr-f2e",
  "version": "1.0.0",
  "description": "React SSR F2E Project",
  "main": "./lib/main.js",
  "scripts": {
    "test": "npm run build",
    "dev": "BABEL_ENV=development webpack-dev-server --progress --open",
    "bs": "BABEL_ENV=development mkdir -p lib && babel src/common -d lib && babel src/server -d lib",
    "bc": "BABEL_ENV=production NODE_ENV=production webpack --progress --config webpack.production.config.js",
    "build": "npm run bc && npm run bs",
    "start": "NODE_SSR=true node lib/main.js",
    "check": "find src | grep \.js | xargs eslint"
  },
  "keywords": [
    "React",
    "SpringHack"
  ],
  "author": "SpringHack",
  "license": "GPL-3.0",
  "dependencies": {
    "body-parser": "^1.17.1",
    "compression": "^1.6.2",
    "cookie-parser": "^1.4.3",
    "express": "^4.15.2",
    "express-session": "^1.15.2",
    "mobx": "^3.1.9",
    "mobx-react": "^4.1.8",
    "multer": "^1.3.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-router-dom": "^4.1.0",
    "babel-polyfill": "^6.23.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-env": "^1.3.3",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.0",
    "cssnano": "^3.10.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "html-webpack-plugin": "^2.28.0",
    "less": "^2.7.2",
    "less-loader": "^4.0.3",
    "postcss-loader": "^1.3.3",
    "style-loader": "^0.16.1",
    "webpack": "^2.3.3",
    "webpack-dev-server": "^2.4.2"
  }
}
